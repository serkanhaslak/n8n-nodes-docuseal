{
  "name": "n8n-nodes-docuseal",
  "version": "0.2.4",
  "description": "n8n nodes to integrate with DocuSeal for document signing and form automation",
  "keywords": [
    "n8n-community-node-package",
    "docuseal",
    "document-signing",
    "forms",
    "pdf"
  ],
  "license": "MIT",
  "homepage": "https://www.docuseal.co",
  "author": {
    "name": "Serkan Haslak",
    "email": "serkan@pragmaticgrowth.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serkanhaslak/n8n-nodes-docuseal.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons && cp -r tools dist/tools",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DocusealApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Docuseal/DocusealApi.node.js",
      "dist/nodes/Docuseal/DocusealTrigger.node.js",
      "dist/nodes/Docuseal/DocusealAiTool.node.js",
      "dist/nodes/Docuseal/DocusealToolAgent.node.js"
    ],
    "tools": [
      "dist/tools/DocusealAITool.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.82.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  }
}
